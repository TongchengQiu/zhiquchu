{
  "presets": [ "es2015", "stage-0", "react"],

  "plugins": [
    "transform-runtime",
    "transform-class-properties",
    "transform-function-bind",
    "transform-decorators-legacy",
    ["antd", { "style": "css" }]
  ],

  "env": {
    "build": {
      "optional": ["optimisation", "minification"]
    }
  }
}
